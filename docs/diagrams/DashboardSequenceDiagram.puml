@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant "e:EventCommand" as EventCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

box UI LOGIC_COLOR_T1
participant "d:SummaryDashboard" as SummaryDashboard LOGIC_COLOR
end box

[-> LogicManager : execute("event 1 t/Interview ...")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("event 1 ...")
activate AddressBookParser

create EventCommand
AddressBookParser -> EventCommand : new EventCommand(...)
activate EventCommand
EventCommand --> AddressBookParser : e
deactivate EventCommand

AddressBookParser --> LogicManager : e
deactivate AddressBookParser

LogicManager -> EventCommand : execute(m)
activate EventCommand

EventCommand -> Model : setPerson(person, personWithNewEvent)
activate Model

Model --> SummaryDashboard : [ListChange notification]
deactivate Model
activate SummaryDashboard

SummaryDashboard -> SummaryDashboard : updateDashboard()
activate SummaryDashboard
SummaryDashboard -> SummaryDashboard : findUpcomingEvents()
activate SummaryDashboard
SummaryDashboard --> SummaryDashboard
deactivate SummaryDashboard
SummaryDashboard -> SummaryDashboard : (update UI Labels)
SummaryDashboard --> SummaryDashboard
deactivate SummaryDashboard

deactivate SummaryDashboard

create CommandResult
EventCommand -> CommandResult
activate CommandResult

CommandResult --> EventCommand
deactivate CommandResult

EventCommand --> LogicManager : r
deactivate EventCommand

[<-- LogicManager : r
deactivate LogicManager
@enduml